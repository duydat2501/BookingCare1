eact-redux\";\r\nimport { push } from \"connected-react-router\";\r\nimport * as actions from \"../../store/actions\";\r\nimport \"./Login.scss\";\r\nimport { FormattedMessage } from \"react-intl\";\r\nimport { handleLoginApi } from \"../../services/userService\";\r\n\r\nclass Login extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      username: \"\",\r\n      password: \"\",\r\n      isShowPassword: false,\r\n      errMessage: \"\",\r\n    };\r\n  }\r\n  handleOnChangeUsername = (event) => {\r\n    this.setState({\r\n      username: event.target.value,\r\n    });\r\n  }; //lưu sự thay đổi của username\r\n  handleOnChangePassword = (event) => {\r\n    this.setState({\r\n      password: event.target.value,\r\n    });\r\n  }; //lưu sự thau đổi của password\r\n\r\n  handleLogin = async () => {\r\n    this.setState({\r\n      errMessage: \"\",\r\n    });\r\n    try {\r\n      let data = await handleLoginApi(this.state.username, this.state.password);\r\n      if (data && data.errCode !== 0) {\r\n        this.setState({\r\n          errMessage: data.message,\r\n        });\r\n      }\r\n      if (data && data.errCode === 0) {\r\n        this.props.userLoginSuccess(data.user)\r\n        console.log(\"login successful\");\r\n      }\r\n    } catch (error) {\r\n      if (error.response) {\r\n        if (error.response.data) {\r\n          this.setState({ errMessage: error.response.data.message });\r\n        }\r\n      }\r\n    }\r\n  }; //lấy giá trị của username và password\r\n\r\n  handleShowHidePassword = () => {\r\n    this.setState({\r\n      isShowPassword: !this.state.isShowPassword,\r\n    });\r\n  }; //Ẩn hiện password\r\n  render() {\r\n    //JSX\r\n    return (\r\n      <div className=\"login-background\">\r\n        <div className=\"login-container\">\r\n          <div className=\"login-content row\">\r\n            <div className=\"col-12 text-Login\">Login</div>\r\n            <div className=\"col-12 from-group login-input\">\r\n              {/* Đổi username sang TV là đăng nhập cũng như login */}\r\n              <label>UserName :</label>\r\n              <input\r\n                type=\"Text\"\r\n                className=\"form-control\"\r\n                placeholder=\"Enter your username\"\r\n                value={this.state.username}\r\n                onChange={(event) => this.handleOnChangeUsername(event)}\r\n              />\r\n            </div>\r\n\r\n            <div className=\"col-12 from-group login-input\">\r\n              <label>Password :</label>\r\n              <div className=\"custom-input-password\">\r\n                <input\r\n                  type={this.state.isShowPassword ? \"text\" : \"password\"}\r\n                  className=\"form-control\"\r\n                  placeholder=\"Enter your password\"\r\n                  onChange={(event) => {\r\n                    this.handleOnChangePassword(event);\r\n                  }}\r\n                />\r\n                <span\r\n                  onClick={() => {\r\n                    this.handleShowHidePassword();\r\n                  }}\r\n                >\r\n                  <i\r\n                    className={\r\n                      this.state.isShowPassword\r\n                        ? \"far fa-eye\"\r\n                        : \"far fa-eye-slash\"\r\n                    }\r\n                  ></i>\r\n                </span>\r\n              </div>\r\n            </div>\r\n            <div className=\"col-12\" style={{ color: \"red\" }}>\r\n              {this.state.errMessage}\r\n            </div>\r\n\r\n            <div className=\"col-12\">\r\n              <button\r\n                className=\"btn-login\"\r\n                onClick={() => {\r\n                  this.handleLogin();\r\n                }}\r\n              >\r\n                Login\r\n              </button>\r\n            </div>\r\n            <div className=\"col-12\">\r\n              <span className=\"forgot-passwo